{
  "manifest_version": 3,
  "name": "Always New Tab (ANT)",
  "description": "New Tab should always be oppened, so this extension opens it for you! Also it can move tab to the front, close excessive and even have separate New Tabs in different tab groups!",
  "version": "1.4",
  "permissions":
  [
    "tabs",
	  "storage",
	  "nativeMessaging",
    "tabGroups"
  ],
  "action":
  {
    "default_popup": "popup/popup.html",
    "default_icon": "images/Cover128.png"
  },
  "icons":
  {
    "128": "images/Cover/Cover128.png",
    "96": "images/Cover/Cover96.png",
    "64": "images/Cover/Cover64.png",
    "32": "images/Cover/Cover32.png",
    "16": "images/Cover/Cover16.png"
  },
  "background":
  {
    "service_worker": "scripts/background.js"
  }
}